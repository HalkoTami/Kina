<!--<?xml version="1.0" encoding="utf-8"?>-->
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:padding="5dp"
    android:orientation="horizontal"

    android:baselineAligned="false">
    <ImageView
        android:contentDescription="@string/title_tab_library"
        android:id="@+id/bnv_imv_1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="50dp"
        android:rotation="50"
        app:layout_constraintBottom_toTopOf="@id/bnv_txv_1"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:src="@drawable/icon_tab_library"
        />
    <TextView
        android:id="@+id/bnv_txv_1"
        android:textSize="11sp"

        android:text="raiburari"

        app:layout_constraintStart_toStartOf="@id/bnv_imv_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bnv_imv_1"
        app:layout_constraintEnd_toEndOf="@id/bnv_imv_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:layout_marginTop="2dp"
        />
    <ImageView
        android:contentDescription="@string/title_tab_create"
        android:id="@+id/imv_edit_file"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/bnv_imv_1"
        app:layout_constraintEnd_toStartOf="@id/imv_3"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="30dp"
        android:layout_height="0dp"
        android:src="@drawable/icon_add"/>
    <ImageView
        android:contentDescription="@string/title_tab_anki"
        android:id="@+id/imv_3"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/txv_3"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:src="@drawable/icon_tab_anki"
        android:layout_marginEnd="50dp"
        />
    <TextView
        android:textSize="11sp"
        android:id="@+id/txv_3"
        app:layout_constraintStart_toStartOf="@id/imv_3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imv_3"
        app:layout_constraintEnd_toEndOf="@id/imv_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:layout_marginTop="2dp"
        />
