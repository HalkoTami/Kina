<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="progress"
            type="Integer" />
        <variable
            name="a"
            type="float" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLay_progressbar_remembered"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <ProgressBar
            android:layout_margin="2dp"
            android:layout_gravity="center_vertical"

            android:id="@+id/progressbar_remembered"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:max="100"
            android:indeterminateOnly="false"
            android:progress="@{progress}"
            android:background="@drawable/rectangle_progressbar"
            android:backgroundTint="@color/light_gray"
            android:progressTint="@color/light_green"
            android:progressDrawable="@drawable/custom_progress_bar_rectangle"
            />
        <ImageView

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            horizontal_constraint_percentage="@{progress}"
            android:id="@+id/imv_remembered_end_icon"
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:src="@drawable/icon_remembered_selectable"
            tools:ignore="ContentDescription"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>